
    <link rel="stylesheet" type="text/css" href="http://tympanus.net/Blueprints/NestedAccordion/css/component.css" />
    <%= stylesheet_link_tag "default.css" %>
    <%= stylesheet_link_tag "bootstrap.css" %>
    <%= stylesheet_link_tag "landing-page.css" %>
    <!-- Bootstrap core CSS -->
    <!-- <link href="http://startbootstrap.com/templates/landing-page/css/bootstrap.css" rel="stylesheet"> -->

    <!-- Custom Google Web Font -->
    <link href="http://startbootstrap.com/templates/landing-page/font-awesome/css/font-awesome.min.css" rel="stylesheet">
    <link href='http://fonts.googleapis.com/css?family=Lato:100,300,400,700,900,100italic,300italic,400italic,700italic,900italic' rel='stylesheet' type='text/css'>


    <nav class="navbar navbar-default navbar-fixed-top" role="navigation">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-ex1-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="http://localhost:3000">CODE UNION</a>
            </div>

            <!-- Collect the nav links, forms, and other content for toggling -->
            <div class="collapse navbar-collapse navbar-right navbar-ex1-collapse">
                <ul class="nav navbar-nav">
                    <li>
                        <% if current_user %>
                    </li>
                    <li><%= link_to "Sign Out", signout_path %>
                    </li>
                    <li><%= link_to "My Account", account_index_path %>
                    </li>
                        <% else %>
                        <%= link_to "Sign in with Github", "/auth/github" %>
                        <% end %>
                    </li>
                    <li><%= link_to "Home", root_path %>
                    </li>
                    <!-- <li><a href="#about">About</a>
                    </li>
                    -->
                </ul>
            </div>
            <!-- /.navbar-collapse -->
        </div>
        <!-- /.container -->
    </nav>
    <div class="intro-header" id="intro-header">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <%= flash[:notice] %><%= flash[:error] %>
                    <div class="intro-message">
                        <h1><%= current_user.name %>'s Account</h1>
                        <hr class="intro-divider">
                    </div>
                </div>
            </div>
        </div>
        <!-- /.container -->
    </div>
    <!-- /.intro-header -->
    <div class="content-section-b">
        <div class="container">
                <div class="col-lg-3">
                    <% if current_user %>
                        <%= link_to image_tag current_user.image, :id => "resize2" %><br/>
                        Username: <%= current_user.nickname %><br/>
                        Email: <%= current_user.email %><br/>
                        Joined: <%= current_user.created_at %><br/>
                    <% end %>
                    <br>
                    <ul class="list-inline intro-social-buttons">
                        <li><a href="/project/new" class="btn btn-default btn-lg"><i class="fa fa-plus-square fa-fw"></i> <span class="network-name">Add Project</span></a>
                        </li>
                    </ul>
                </div>
                <div class="col-lg-9" id="scrolly">
                    <ul>
                        <% @projects.each do |project| %>
                            <h3><%= project.name %><span><a href="/dashboard/<%= project.id %>"><i class="fa fa-tachometer pull-right"></i></a><a href="/project/<%= project.id %>/edit"><i class="fa fa-gear pull-right"></i></a><%= link_to "", project_path(project.id), method: :delete, :class => "fa fa-times pull-right" %></span></h3><hr>
                            <div>
                                Github Repo:  <%= project.github %><br/>
                                Pivotal Project:  <%= project.pivotal %><br/>
                                Code Climate Repo:  <%= project.code %><br/>
                                Travis Ci Repo:  <%= project.travis_ci %>
                            <% end %>
                            </div>
                    </ul> 
                </div>
            <!-- </div> -->
            <div id="pagination">
            <center><%= paginate @projects %></center>
            </div>
        </div>
        <!-- /.container -->
    </div>
    <!-- /.content-section-b -->
    <div class="content-section-a">
        <div class="container">
            <div class="row">
            </div>
        </div>
        <!-- /.container -->
    </div>
    <!-- /.content-section-a -->
    <div class="banner" id="account-banner">
        <div class="container">
            <div class="row">
            </div>
        </div>
        <!-- /.container -->
    </div>
    <footer>
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <ul class="list-inline">
                        <li><%= link_to "Home", root_path %>
                        </li>
                        <li class="footer-menu-divider">&sdot;</li>
                        <li><a href="/about">About</a>
                        </li>
                        <li class="footer-menu-divider">&sdot;</li>
                        <li><a href="/services">Services</a>
                        </li>
                        <li class="footer-menu-divider">&sdot;</li>
                        <li><a href="/contact">Contact</a>
                        </li>
                    </ul>
                    <p class="copyright text-muted small">Copyright &copy; Code Union 2014. All Rights Reserved</p>
                </div>
            </div>
        </div>
    </footer>

    <!-- JavaScript -->
    <script src="http://startbootstrap.com/templates/landing-page/js/jquery-1.10.2.js"></script>
    <script src="jhttp://startbootstrap.com/templates/landing-page/js/bootstrap.js"></script>

    
